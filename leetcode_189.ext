class Solution {
    public void rotate(int[] nums, int k) {
        int n=nums.length;
        int c[]=new int[n];
        int j=-1;
        k=k%n;
        for(int l=n-k;l<n;l++)
        {
            c[++j]=nums[l];
        }
        for(int l=0;l<n-k;l++)
        {
            c[++j]=nums[l];
        }
        for(int i=0;i<n;i++)
        {
            nums[i]=c[i];
        }
        
        
    }
}