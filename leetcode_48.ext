class Solution {
    public void rotate(int[][] matrix) {
        int copy[][]=new int[matrix.length][];
        for(int i=0;i<matrix.length;i++)
        {
            copy[i]=matrix[i].clone();
        }
        for(int i=0;i<matrix.length;i++)
        {
            for(int j=0;j<matrix.length;j++)
            {
                matrix[i][j]=copy[matrix.length-1-j][i];
            }
        }

    }
}